{
  "manifest_version": 2,
  "name": "PassManage",
  "version": "1.0.0",
  "description": "A chrome extension to manage your passwords locally and securely.",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_security_policy": "script-src 'self' https://cdn.jsdelivr.net https://cdnjs.cloudflare.com; object-src 'self'",
  "web_accessible_resources": [
    "index.html",
    "background.js",
    "bulma.min.css"
  ]
}